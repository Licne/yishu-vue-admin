{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*.ts",
    "src/**/*",
    "src/**/*.vue",
    "vite-env.d.ts",
    "src/typings/**/*.d.ts", // 引入全局环境接口interface
      ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": [
      "element-plus/global"
    ]
  },
  
}
